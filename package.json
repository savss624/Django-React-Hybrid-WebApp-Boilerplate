{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clear": "rm -rf ./fe-apps/build ./apps/static/build",
    "django-tailwind-watch": "npx tailwindcss -c ./tailwind.config.django.js -i fe-apps/src/styles/tailwind.css -o ./apps/static/build/css/tailwind.django.css --watch",
    "django-tailwind": "npx tailwindcss -c ./tailwind.config.django.js -i fe-apps/src/styles/tailwind.css -o ./apps/static/build/css/tailwind.django.css",
    "dev": "webpack --config fe-apps/webpack/webpack.config.dev.js",
    "start": "npm run clear && npm run django-tailwind-watch & npm run dev",
    "prod": "webpack --config fe-apps/webpack/webpack.config.prod.js",
    "build": "cross-env NODE_ENV=production npm run clear && npm run django-tailwind && npm run prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.23.0",
    "eslint-config-react-app": "^7.0.1",
    "glob": "^8.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-loader": "^7.0.1",
    "tailwindcss": "^3.1.8",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "private": true
}
